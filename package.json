{
  "scripts": {
    "start": "npm run watch:eleventy & npm run watch:typescript & npm run watch:sass",
    "build:sass": "sass src/styles/styles.scss docs/assets/styles.css",
    "watch:sass": "sass --watch src/styles/styles.scss docs/assets/styles.css",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build:eleventy": "npx @11ty/eleventy",
    "build:typescript": "npx tsc src/scripts/main.ts --outfile docs/main.js",
    "watch:typescript": "npx --watch tsc src/scripts/main.ts --outfile docs/main.js",
    "build": "npm run build:eleventy & npm run build:typescript & npm run build:sass"
  },
  "dependencies": {
    "bootstrap": "5.1.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.0-beta.10",
    "nunjucks": "3.2.3",
    "sass": "1.45.1",
    "typescript": "4.5.4"
  }
}