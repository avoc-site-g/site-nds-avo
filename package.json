{
  "scripts": {
    "start": "npm-run-all --parallel watch:*",
    "build:sass": "sass --style=compressed src/styles/styles.scss docs/assets/styles.css",
    "watch:sass": "sass --style=compressed --watch src/styles/styles.scss docs/assets/styles.css",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build:eleventy": "npx @11ty/eleventy",
    "build:typescript": "npx tsc src/scripts/main.ts --outfile docs/assets/main.js",
    "watch:typescript": "npx --watch tsc src/scripts/main.ts --outfile docs/main.js",
    "build": "npm-run-all build:*"
  },
  "dependencies": {
    "bootstrap": "5.1.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.0-beta.10",
    "@types/node": "17.0.8",
    "html-minifier": "4.0.0",
    "liquidjs": "9.32.1",
    "npm-run-all": "4.1.5",
    "sass": "1.45.1",
    "typescript": "4.5.4"
  }
}